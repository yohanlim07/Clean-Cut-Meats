	mc:Ignorable="d"
	x:Class="GUI2FullPrototypeScreens.navbar"
	x:Name="UserControl"
	d:DesignWidth="170" d:DesignHeight="748" Height="748" Width="170">

	<Grid x:Name="LayoutRoot">
